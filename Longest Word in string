class HelloWorld {
    public static void main(String[] args) {
        
        String s="hi MR purnachandar";
        String result="";
        String[] y=s.split(" ");
       
        
        
        for(String x:y) {
            if(x.length()>result.length()) {
                result=x;
            }
        }
        System.out.println(result);
        
        
        
    }
}
