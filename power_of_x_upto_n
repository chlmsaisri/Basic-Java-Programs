import java.util.* ;
import java.io.*; 

class Solution {
    public static void main(String args[]) {
        // Write code here
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();
        int pow = 1;
        for (int i = 1; i <= n; i++) {
            pow *= x;
        }
        System.out.println(pow);
    }
}
